
init = function() {
  enqueueDia(new Dia(DialogueFace.MARTY, "That cruel Farmer Blue has done it again!", ThatCruelFarmerBlue));
  enqueueDia(new Dia(DialogueFace.MARTY, "I have to get to his house as quickly as possible.", IHaveToGetTo));
  enqueueDia(new Dia(DialogueFace.MARTY, "I know! I'll use the arrow keys to move.", IKnowIllUseTheArrow));
